{
	"compileOnSave": false,
	"compilerOptions": {
		"outDir": "./dist/out-tsc",
		"baseUrl": "./",
		"noImplicitAny": false,
		"noEmitOnError": true,
		"removeComments": false,
		"sourceMap": true,
		"target": "es5",
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"declaration": false,
		"paths": {
			"@spa/app*": ["app*"]
		},
		"typeRoots": [
			"node_modules/@types"
		],
		"lib": [
			"es2016",
			"dom"
		]
	}
}
