<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>netcoreapp2.0;net461</TargetFrameworks>
    <Description>A basic PDF document generation library for .Net Core</Description>
    <Authors>CorePDF Contributors</Authors>
    <Company />
    <Version>0.4.0-beta</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>2017 CorePDF Contributors</Copyright>
    <PackageProjectUrl>https://github.com/GZidar/CorePDF</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/GZidar/CorePDF#license</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/GZidar/CorePDF</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>pdf-generation pdf .NETCore</PackageTags>
    <PackageIconUrl>https://raw.githubusercontent.com/GZidar/CorePDF/development/src/corepdf.png</PackageIconUrl>
    <AssemblyVersion>0.4.0.0</AssemblyVersion>
    <FileVersion>0.4.0.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
    <PackageReference Include="SharpZipLib.NETStandard" Version="1.0.7" />
    <PackageReference Include="SixLabors.ImageSharp" Version="1.0.0-beta0002" />
  </ItemGroup>

</Project>
