<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp2.0</TargetFramework>
    <Description>A basic PDF document generation library for .Net Core</Description>
    <Authors>CorePDF Contributors</Authors>
    <Company />
    <Version>0.1.0-alpha</Version>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Copyright>2017 CorePDF Contributors</Copyright>
    <PackageProjectUrl>https://github.com/GZidar/CorePDF</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/GZidar/CorePDF#license</PackageLicenseUrl>
    <RepositoryUrl>https://github.com/GZidar/CorePDF</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PackageTags>pdf-generation pdf .NETCore</PackageTags>
    <PackageIconUrl>https://github.com/GZidar/CorePDF/src/corepdf.png</PackageIconUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.1" />
    <PackageReference Include="SharpZipLib.NETStandard" Version="0.86.0.1" />
    <PackageReference Include="SixLabors.ImageSharp" Version="1.0.0-beta0001" />
  </ItemGroup>

</Project>
